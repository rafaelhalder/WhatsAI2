    "@sentry/node": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
